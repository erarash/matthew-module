\copy comments("user", "prodRating", "yesRating", "noRating", date, body, verified, recommend, size, width, comfort, quality, response, "prodId", header) FROM '/home/arash/HRLA25/SDC/matthew-module/database/seed/seededData.csv' WITH DELIMITER ',' CSV HEADER;


CREATE TABLE comments(
 id serial PRIMARY KEY,
 "user" character VARYING(254) NOT NULL,
 "prodRating" smallint,
 "yesRating" smallint NOT NULL,
 "noRating" smallint NOT NULL,
 "date" DATE NOT NULL,
 "body" TEXT NOT NULL,
 "verified" BOOLEAN NOT NULL,
 "recommend" BOOLEAN NOT NULL,
 "size" smallint NOT NULL,
 "width" smallint NOT NULL,
 "comfort" smallint NOT NULL,
 "quality" smallint NOT NULL,
 "response" TEXT,
 "prodId" smallint NOT NULL,
 "header" TEXT
);

